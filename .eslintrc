{
    "rules": {
        "indent": [ 2, 2 ],
        "id-length": [2, {
            "min": 3, "max": 30,
            "properties": "never",
            "exceptions": [
                "$",
                "i"
            ]
        }],
        new-cap: ["error", { "capIsNewExceptions": ["Module"] }],
        "no-console": 0
    },
    "env": {
        "node": true,
        "jquery" : true
    },
    "globals": {
        "Module": true
    },
    "extends": "warner"
}
